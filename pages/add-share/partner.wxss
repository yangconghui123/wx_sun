/* pages/add-share/partner.wxss */
page{
width:100%;
height:100%;
overflow: hidden;
}
.backg_round{
  height:100%;
  width:100%;
   display:flex;
  justify-content: center;
  align-items:center;
   /* text-align:center;
line-height:100%; */
  /* background-image: url(''); */
}
.backgroundImage{
  height:100%;
  width:100%;
  position:absolute;
  background-size:100%;
}
.bonus{
  position:relative;
font-size:80rpx;
color:rgba(255,251,196,1) ;
z-index:9999;
/* top:5rpx;rgba(255,251,196,1) */
}
.bonus_rule{
  position:absolute;
  bottom:38rpx;
  font-size:28rpx;
font-family:Microsoft YaHei;
font-weight:400;
color:rgba(255,255,255,1);
}
.bonus_price{
  font-size:100rpx;
font-family:Microsoft YaHei;
font-weight:bold;
color:rgba(255,251,196,1) ;
line-height:36px;
}
.shadow{
  width: 100%;
  height: 100%;
  z-index: 80;
  position: absolute;
  top: 0;
  background-color: #000;
  opacity: 0.5;
}
/* 上滑高度 */
.choosen{
  width: 100%;
  /* height: 80%;  */
  position: fixed;
  bottom: 0;
  background-color: #fff;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  z-index: 98;
}
/* 内容 */
.container-box {
  display: flex;
  height: 100%;
  flex-direction:row;
  flex-wrap:wrap;
  width: 100%;
  border-radius: 15rpx;
  padding:30rpx;
  z-index: 98;
  line-height:60rpx;
}
 
/* 遮罩结束 */
 